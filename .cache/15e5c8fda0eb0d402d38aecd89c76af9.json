 .item stuff\r\n    .item kek\r\n  //ui-sidebar(:show=\"sidebarIsOpen\" attach=\"left\" inverted vertical)\r\n  //.pusher\r\n    h1 {{page}}\r\n    ui-button(primary @click=\"openSidebar\") Open sidebar\r\n</template>-->\r\n\r\n<script>\r\nimport PropTypes from \"../editor/propTypes\";\r\n\r\nexport default {\r\n  name: \"Editor\",\r\n  data() {\r\n    return {\r\n      data: [],\r\n      focused: null\r\n    };\r\n  },\r\n  created() {\r\n    const { page } = this;\r\n    const { token } = this.$auth;\r\n\r\n    // Call api function to get page information\r\n    this.$api(`/edit/${page}`, { body: { token } })\r\n      .then(data => {\r\n        this.data = data;\r\n      }).catch(console.error);\r\n  },\r\n  computed: {\r\n    page() {\r\n      return this.$route.params.page;\r\n    }\r\n  },\r\n  methods: {\r\n    openSidebar() {\r\n      this.sidebarIsOpen = !this.sidebarIsOpen;\r\n    },\r\n    addComponent(e) {\r\n      const {target} = e;\r\n\r\n      if(target.matches(\"a.item\")) {\r\n        const name = target.textContent;\r\n        alert(`Added a '${name}' component. Poof!`);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>"},"lineCount":null}},"hash":"68e9e7e3a00edb83778d6fd430647d28","cacheData":{"env":{}}}